<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/oredev-kodapor.css" />
  </head>
  <body>
    <nav>
      <ul>
        <%pages.forEach(function (page) {%>
          <li <%if(page.active) {%>class="active"<%}%>><a href="<%=page.href%>"><%=page.name%></a>
        <%})%>
      </ul>
    </nav>
    <h1><%=title%></h1>
    <div id="chart"><svg /></div>
    <script src="oredev-kodapor.js"></script>
    <script>
      barAndLine([
        {
          key: 'Growth',
          bar: true,
          color: '#ccf',
          values: [
            <%members.forEach(function (member) {%>
              ['<%=member.year+'-'+member.month%>', <%=member.increase%>],
            <%})%>
          ]
        },
        {
          key: 'Members',
          color: '#000',
          values: [
            <%members.forEach(function (member) {%>
              ['<%=member.year+'-'+member.month%>', <%=member.total%>],
            <%})%>
          ]
        }
      ]);
    </script>
    <script 
  </body>
</html>
